<%- include('partials/header'); -%>

    <div class="container">
        <div class="row">
            <% tv.forEach(function(tv){ %>
                <div class="col-md-4 mb-3">
                    <div class="card">
                        <img src="<%= tv.image %>" alt="">
                        <div class="card-body">
                            <div class="card-title"><%= tv.name %></div>
                        </div>
                    </div>
                </div>
            <%  }) %>
            <div class="col-md-4">

            </div>
        </div>
    </div>

<%- include('partials/footer'); -%>